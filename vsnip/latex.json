{
  "cv_Section": {
    "prefix": ["\\cv_section"],
    "body": ["\\section{$1}$0"],
    "description": "section in moderncv."
  },
  "cv_Subsection": {
    "prefix": ["\\cv_subsection"],
    "body": ["\\subsection{<< $1 >>}$0"],
    "description": "Insert a new moderncv."
  },
  "cv_item": {
    "prefix": ["\\cv_item"],
    "body": ["\\cvitem{$1}{$2}$0"],
    "description": "cvitem in moderncv."
  },
  "cv_listitem": {
    "prefix": ["\\cv_listitem"],
    "body": ["\\cvlistitem{$1}$0"],
    "description": "cvlistitem in moderncv."
  },
  "cv_entry": {
    "prefix": ["\\cv_entry"],
    "body": ["\\cventry{$1}{$2}{$3}{$4}{$5}{$6}$0"],
    "description": "cventry in moderncv."
  },
  "tilde": {
    "prefix": ["~"],
    "body": ["\\$\\sim\\$"],
    "description": "escape ~"
  },
  "backslash": {
    "prefix": ["\\"],
    "body": ["\\$\\backslash\\$"],
    "description": "escape \\"
  },
  "open brace": {
    "prefix": ["{"],
    "body": ["\\{"],
    "description": "escape {"
  },
  "close brace": {
    "prefix": ["}"],
    "body": ["\\\\}"],
    "description": "escape }"
  },
  "caret": {
    "prefix": ["^"],
    "body": ["\\^"],
    "description": "escape ^"
  },
  "ampersand": {
    "prefix": ["&"],
    "body": ["\\&"],
